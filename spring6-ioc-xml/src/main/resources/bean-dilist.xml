<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="employeeOne" class="com.example.spring6.iocxml.ditest.Employee">
        <property name="employeeName" value="Mary"></property>
        <property name="age" value="20"></property>
    </bean>

    <bean id="employeeTwo" class="com.example.spring6.iocxml.ditest.Employee">
        <property name="employeeName" value="Andy"></property>
        <property name="age" value="25"></property>
    </bean>

    <bean id="department" class="com.example.spring6.iocxml.ditest.Department">
        <property name="departmentName" value="Apple"></property>
        <property name="employeeList">
            <list>
                <ref bean="employeeOne"></ref>
                <ref bean="employeeTwo"></ref>
            </list>
        </property>
    </bean>

</beans>